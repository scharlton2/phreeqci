200   punch eol$ + 'SURFACE_MIX 5'
210   punch eol$ + ' 1 ' + str_e$(x1, 8, 5) + sc$ + ' 2 ' + str_e$(x2, 8, 5)
220   if x1 > 0.52 then punch eol$ + 'COPY SOLID_SOLUTIONS 2 5' else punch eol$ + 'COPY SOLID_SOLUTIONS 1 5'
230   punch eol$ + 'END'
 
 
300   punch eol$ + 'USE solution 5'
