DATABASE ../database/pitzer.dat
SELECTED_OUTPUT 101
    -file                 pitzer_redef_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 PUNCH STR_F$(MU, 20, 12)
20 PUNCH STR_F$(SC, 20, 10)
    -end
PITZER
# -MacInnes false
-B0
   Cl-    K+   0.04835       0           0           5.794E-4
  # Cl-    K+  0.04808 # -758.48  -4.7062 0.010072  -3.7599e-6
  # Cl-    K+  0.04808 # -758.48  -4.7062 0.010072  -3.7599e-6
SOLUTION 11
    temp 25
 pH 7 charge
    Na 3824
 Ca 1.8500e+001
 S(6) 1.9305e+003
        END

