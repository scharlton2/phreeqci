	 -axis_scale x_axis 0 500
	 10 x = pressure
	 12 if x > 500 then goto 400
	 20 graph_x x
	 30 graph_y pr_phi("H2O(g)"), pr_phi("CO2(g)"), pr_phi("N2(g)"), pr_phi("CH4(g)")
	 40 graph_sy sys("C"), sys("N")
	400 end
	USER_GRAPH 2 Gas pressures
