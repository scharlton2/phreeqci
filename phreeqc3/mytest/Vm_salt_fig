120 plot_xy mu^0.5, Vmol, symbol = None  , color = Red, line_width = 2
122 if step_no > 2 then goto 150
134 if step_no = 1 then plot_xy 0, Vmol, symbol = None, color = Red, line_width = 1 else\
                        plot_xy 1, get(11) + DH_Av, symbol = None, color = Red, line_width = 1
140 put(Vmol, 11)
150 end
END

