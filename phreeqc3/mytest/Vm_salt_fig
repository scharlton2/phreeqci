USE solution 1
REACTION 1; HCl 1; 1e-6 5*0.002 4*0.02 4*0.2 5*0.5 7*1
INCREMENTAL_REACTIONS true
USER_GRAPH
52 Vmol = Vm("Cl-")
121 plot_xy mu^0.5, Vmol, symbol = None, color = Blue, line_width = 2
122 if step_no > 2 then goto 150
134 if step_no = 1 then plot_xy 0, Vmol, symbol = None, color = Blue, line_width = 1 else\
                        plot_xy 1, get(11) + DH_Av * 1 * 0.5, symbol = None, color = Blue, line_width = 1
140 put(Vmol, 11)
150 end
END

