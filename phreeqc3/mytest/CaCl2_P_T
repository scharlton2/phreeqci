DATABASE ../database/pitzer.dat
SELECTED_OUTPUT 101
    -file                 CaCl2_P_T_101.sel
USER_PUNCH 101
    -headings Mu SC Viscosity
    -start
10 PUNCH STR_F$(MU, 20, 12)
20 PUNCH STR_F$(SC, 20, 10)
30 PUNCH STR_F$(VISCOS, 20, 10)
    -end
# Data from Laliberte's database
SOLUTION
pH 7 charge
REACTION_TEMPERATURE 1; 0
REACTION 1; CaCl2 1
0.004, 0.199, 0.513, 0.709, 1.021, 1.546, 2.084, 2.676, 3.281, 3.872, 4.290, 4.483, 4.802, 5.252, 5.475, 5.783
PRINT; -reset false; -user_print true
# SELECTED_OUTPUT; -reset false; -file in2.prn; -high_precision true
USER_GRAPH
-headings 0°C
-axis_scale x_axis 0 3
-axis_scale y_axis 0 4
-axis_titles "CaCl2 / (mol/kgw)" "viscosity / (mPa•s)"
200 data 1.811, 1.885, 2.017, 2.106, 2.279, 2.656, 3.086, 3.792, 4.763, 6.028, 7.252, 7.999, 9.811, 11.490, 12.970, 15.230
210 restore 200 : erase v_ : dim v_(500) : for i = 1 to step_no : READ v_(i) : next i
300 plot_xy tot("Ca"), v_(step_no ), color = Red, line_width = 0
310 plot_xy tot("Ca"), viscos ,      color = Red, symbol_size = 0
400 dev = (v_(step_no ) - viscos) / viscos 
410 if step_no = 1 then  put(0, 1) : put(0, 2) : put(0, 3) : put(0, 4)
420 if step_no > 0 then  put(dev^2 + get(1), 1) : put(1 + get(2), 2)
430 if step_no = 16 then print 's.d. for viscos of CaCl2, 0°C', (get(1) / get(2) )^0.5 * 1e2 , '%'
END
 
