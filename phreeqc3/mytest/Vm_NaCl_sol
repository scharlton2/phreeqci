# calculates and plots molar volumes of NaCl in solution (c, tc)
# data from NaCl.xls, Laliberte, 2009.
DATABASE ../database/phreeqc.dat
SELECTED_OUTPUT 101
    -file                 Vm_NaCl_sol_101.sel
USER_PUNCH 101
    -headings Mu SC
    -start
10 PUNCH STR_F$(MU, 20, 12)
20 PUNCH STR_F$(SC, 20, 10)
    -end
#PRINT; -reset false

SOLUTION 1; pH 7 charge
END

USE solution 1
REACTION_TEMPERATURE; 5.000000
