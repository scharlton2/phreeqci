360   if equi("Cl_AFmsura") > 0 then punch eol$ + 'USE surface 5'
362   punch eol$ + 'REACTION_TEMPERATURE 5' + sc$ + ' 25'
370   punch eol$ + 'END'
400 next i
END
 
