	 20 graph_x x
	 30 graph_y pr_p("H2O(g)"), pr_p("CO2(g)"), pr_p("N2(g)"), pr_p("CH4(g)")
	 40 graph_sy tot("C(4)"), tot("N(0)"), tot("C(-4)")
	400 print 'temp (oC) = ', trim(str$(tc)), '. step ', trim(str$(step_no))
