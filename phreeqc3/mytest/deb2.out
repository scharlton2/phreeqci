	20 PUNCH STR_F$(SC, 20, 10)
	    end
	KNOBS
	 iterations 400
	SURFACE_MASTER_SPECIES
	 Su Su-
	SURFACE_SPECIES
	 Su- = Su-
	 log_k 0
